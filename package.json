{
  "name": "histograph-data-tgn",
  "version": "0.6.0",
  "description": "Download and convert TGN data to Histograph NDJSON",
  "main": "tgn.js",
  "devDependencies": {
    "jscs": "^1.10.0",
    "jshint": "^2.6.0",
    "tape": "^3.4.0"
  },
  "scripts": {
    "test": "jshint *.js && jscs *.js"
  },
  "author": "Bert Spaan",
  "license": "MIT",
  "dependencies": {
    "async": "^1.4.2",
    "sparql-client": "^0.2.0",
    "xml2js": "^0.4.13"
  },
  "jshintConfig": {
    "node": true,
    "globalstrict": false,
    "undef": true,
    "unused": true,
    "noarg": true
  },
  "jscsConfig": {
    "preset": "airbnb",
    "requireCamelCaseOrUpperCaseIdentifiers": null,
    "disallowMultipleVarDecl": true,
    "requireMultipleVarDecl": null
  }
}
